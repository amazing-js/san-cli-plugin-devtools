        "url": "https://github.com/ecomfe/san-cli/issues"
    },
    "homepage": "https://ecomfe.github.io/san-cli",
    "dependencies": {
        "execa": "^4.1.0",
        "san-cli-utils": "^1.0.0",
        "san-devtools": "^3.0.3",
        "tiny-emitter": "^2.1.0",
        "whatwg-fetch": "^3.5.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@babel/preset-env": "^7.12.7",
        "@ecomfe/eslint-config": "^3.2.0",
        "autoprefixer": "^9.7.5",
        "babel-plugin-import": "^1.13.3",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.2",
